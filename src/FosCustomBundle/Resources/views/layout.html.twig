<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    {% stylesheets '@css_includes' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
</head>
<body>


{% if app.request.hasPreviousSession %}
    {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
            <div class="flash-align">
                <h2>{{ message }}</h2>
            </div>
        {% endfor %}
    {% endfor %}
{% endif %}

<div class="form-group login-align">
    {% block fos_user_content %}
    {% endblock fos_user_content %}
</div>
{% block javascripts %}
    {% javascripts '@js_libs' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
</body>
</html>



